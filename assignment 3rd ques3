#include <bits/stdc++.h>

using namespace std;

int main(){
    int j=0,k=0,temp;
    vector<int>A;
    for(int i=0;i<3;i++){
        cin>>temp;
        A.push_back(temp); 
    }
    vector<int>B;
    while(cin>>temp){
        B.push_back(temp);
    }
    for(int i=0;i<A.size();i++){
        if(A[i]>B[i]){
              j+=1;
             }
         else if(B[i]>A[i]) {
         k+=1;
         }
    }
    cout<<j<<" ";
    cout<<k;
    return 0;
}

#include <iostream>
#include <vector>
using namespace std;

int main(){
    int n;
    cin>>n;
    long int temp;
    long int c=0;
    vector<int>A;
    for(int i=0;i<n;i++){
        cin>>temp;
        A.push_back(temp);
         }
     for(int i=0;i<n;i++){
         c+=A[i];
     }
    cout<<c;
}
